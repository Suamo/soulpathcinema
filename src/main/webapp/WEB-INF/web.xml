<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>MainAppService</servlet-name>
        <servlet-class>server.MainAppServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ImageServlet</servlet-name>
        <servlet-class>server.ImageServlet</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>MainAppService</servlet-name>
        <url-pattern>/MainApp/MainAppService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ImageServlet</servlet-name>
        <url-pattern>/MainApp/ImageServlet</url-pattern>
    </servlet-mapping>

</web-app>
